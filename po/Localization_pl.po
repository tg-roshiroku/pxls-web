msgstr "Hasło musi zawierać przynajmniej jeden znak specjalny."

#: /views/profile.handlebars
msgid "Enter your current password to confirm changes."
msgstr "Wprowadź swoje aktualne hasło, aby potwierdzić zmiany."

#: /views/profile.handlebars
msgid "Show Password"
msgstr "Pokaż hasło"

#: /views/profile.handlebars
msgid "Hide Password"
msgstr "Ukryj hasło"

#: /views/profile.handlebars
msgid "Account Security"
msgstr "Bezpieczeństwo konta"

#: /views/profile.handlebars
msgid "Two-Factor Authentication"
msgstr "Uwierzytelnianie dwuskładnikowe"

#: /views/profile.handlebars
msgid "Enable 2FA"
msgstr "Włącz 2FA"

#: /views/profile.handlebars
msgid "Disable 2FA"
msgstr "Wyłącz 2FA"

#: /views/profile.handlebars
msgid "Scan this QR code with your authenticator app."
msgstr "Zeskanuj ten kod QR za pomocą aplikacji uwierzytelniającej."

#: /views/profile.handlebars
msgid "Enter the code generated by your app."
msgstr "Wprowadź kod wygenerowany przez aplikację."

#: /views/profile.handlebars
msgid "2FA enabled successfully."
msgstr "Uwierzytelnianie 2FA zostało pomyślnie włączone."

#: /views/profile.handlebars
msgid "2FA disabled successfully."
msgstr "Uwierzytelnianie 2FA zostało pomyślnie wyłączone."

#: /views/profile.handlebars
msgid "Invalid authentication code."
msgstr "Nieprawidłowy kod uwierzytelniający."

#: /views/profile.handlebars
msgid "Backup Codes"
msgstr "Kody zapasowe"

#: /views/profile.handlebars
msgid "Use these codes if you lose access to your authenticator."
msgstr "Użyj tych kodów, jeśli utracisz dostęp do aplikacji uwierzytelniającej."

#: /views/profile.handlebars
msgid "Keep them somewhere safe!"
msgstr "Przechowuj je w bezpiecznym miejscu!"

#: /views/profile.handlebars
msgid "Regenerate Backup Codes"
msgstr "Wygeneruj ponownie kody zapasowe"

#: /views/profile.handlebars
msgid "Codes regenerated."
msgstr "Kody zostały ponownie wygenerowane."

#: /views/profile.handlebars
msgid "Are you sure you want to regenerate your backup codes?"
msgstr "Czy na pewno chcesz ponownie wygenerować swoje kody zapasowe?"

#: /views/profile.handlebars
msgid "Previous codes will no longer work."
msgstr "Poprzednie kody nie będą już działać."

#: /views/profile.handlebars
msgid "Download Codes"
msgstr "Pobierz kody"

#: /views/profile.handlebars
msgid "Copy Codes"
msgstr "Skopiuj kody"

#: /views/profile.handlebars
msgid "Copied!"
msgstr "Skopiowano!"

#: /views/profile.handlebars
msgid "Error enabling 2FA. Please try again."
msgstr "Błąd podczas włączania 2FA. Spróbuj ponownie."

#: /views/profile.handlebars
msgid "Error disabling 2FA. Please try again."
msgstr "Błąd podczas wyłączania 2FA. Spróbuj ponownie."

